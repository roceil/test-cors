{
  "name": "nuxt3-template",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.12.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:view": "pnpm dlx eslint-flat-config-viewer",
    "add layout": "npx nuxi add layout default",
    "add page": "npx nuxi add page index",
    "add component": "npx nuxi add component demoFolder/BtnBlue",
    "add composable": "npx nuxi add composable demo",
    "add api": "npx nuxi add api auth/demo.get",
    "lint:watch": "nodemon --exec 'npm run lint:fix' --watch . --ext .vue",
    "prepare": "husky"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.3",
    "@nuxt/devtools": "latest",
    "@nuxtjs/color-mode": "^3.3.3",
    "@pinia/nuxt": "^0.5.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@vueuse/nuxt": "^10.9.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "husky": "^9.0.11",
    "nuxt": "^3.11.1",
    "pinia": "^2.1.7",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.1",
    "unplugin-icons": "^0.18.5",
    "unplugin-vue-components": "^0.26.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-tsc": "^2.0.7"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.1.20",
    "@iconify-json/svg-spinners": "^1.1.2",
    "@nuxt/ui": "^2.15.2",
    "nodemon": "^3.1.0",
    "nuxt-icon": "^0.6.10"
  }
}
